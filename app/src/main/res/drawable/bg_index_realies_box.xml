<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="rectangle">
            <corners android:radius="100dp" />
            <gradient
                android:angle="135"
                android:endColor="#FFFFB054"
                android:startColor="#FFFF6EA2" />
        </shape>
    </item>

    <item
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0.5dp">
        <shape android:shape="rectangle">
            <corners android:radius="100dp" />
            <gradient
                android:angle="135"
                android:endColor="#FFFFF0DE"
                android:startColor="#FFFFE3ED" />
        </shape>
    </item>
</layer-list>